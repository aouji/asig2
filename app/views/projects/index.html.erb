	<%= content_for :title, "Projects for YOU" %> 
	<!-- #{current_user.email} -->
<%= link_to "Create new project",new_project_path%>
	<h2>Projects you own</h2>
    <ul>
    <% @owned_projs.each do |owned| %>
      <li>Title: <%= link_to owned.title, project_path(owned) %>
        <ul>
        <li>Description: <%= truncate_s(owned.description,30) %></li>
        <li><%= link_to "Add or remove members",project_members_path(owned) %></li>
        <li><%= button_to "Delete project",project_path(owned),method: :delete, data: {confirm: "Are you sure you want to delete this?"} %></li>
      </ul></li>
    <% end %>
    </ul>
  <h2>Projects you are a member of</h2>
    <ul>
    <% @member_projs.each do |member_in| %>
      <li>Title: <%= link_to member_in.title, project_path(member_in) %></li>
      <li>Description: <%= truncate_s(member_in.description,30) %></li>
    <% end %>
    </ul>